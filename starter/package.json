{
  "name": "myreads",
  "version": "1.0.0",
  "description": "A personal bookshelf app that helps you organize your reading journey. Easily search for books, sort them into categories like Read, Currently Reading, or Want to Read, and keep track of your progress as you explore new stories.",
  "keywords": [
    "udacity",
    "react",
    "myreads"
  ],
  "private": true,
  "author": "Sayyid Maulana Khakul Yakin",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "gh-pages": "^6.2.0"
  }
}
